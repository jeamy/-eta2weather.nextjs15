<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.etaweather.docker-compose</string>
    <key>ProgramArguments</key>
    <array>
        <string>/usr/local/bin/docker-compose</string>
        <string>up</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>WorkingDirectory</key>
    <string>REPLACE_WITH_APP_PATH</string>
    <key>StandardErrorPath</key>
    <string>~/Library/Logs/etaweather-docker-compose.err</string>
    <key>StandardOutPath</key>
    <string>~/Library/Logs/etaweather-docker-compose.log</string>
    <key>KeepAlive</key>
    <true/>
</dict>
</plist>
